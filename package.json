{
  "name": "software-testing",
  "version": "0.1.0",
  "description": "Software Testing Exploration",
  "main": "index.js",
  "scripts": {
    "unit:test": "NODE_ENV=testing tape unit-test/program.test.js",
    "unit:test:solution": "NODE_ENV=testing tape unit-test/solution/program.test.js",
