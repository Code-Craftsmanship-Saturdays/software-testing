    "bdd:test:solution": "NODE_ENV=testing mocha bdd/solution/program.test.js",
    "tdd:test:cycle1": "NODE_ENV=testing tape tdd/solution/tdd-cycle/cycle1/program.test.js",
    "tdd:test:cycle1:silent": "npm run --silent tdd:test:cycle1",
    "tdd:test:cycle2": "NODE_ENV=testing tape tdd/solution/tdd-cycle/cycle2/program.test.js",
    "tdd:test:cycle2:silent": "npm run --silent tdd:test:cycle2",
